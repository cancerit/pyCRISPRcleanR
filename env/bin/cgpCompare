#!/nfs/users/nfs_s/sb43/scripts/archCompare/env/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'archCompare','console_scripts','cgpCompare'
__requires__ = 'archCompare'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('archCompare', 'console_scripts', 'cgpCompare')()
    )
