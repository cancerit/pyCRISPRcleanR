# EASY-INSTALL-ENTRY-SCRIPT: 'cgp-seq-input-val','console_scripts','cgpSeqInputVal'
__requires__ = 'cgp-seq-input-val'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('cgp-seq-input-val', 'console_scripts', 'cgpSeqInputVal')()
    )
